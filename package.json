{
  "name": "thegetcompany-assets",
  "version": "2.2.0",
  "main": "dist/index.js",
  "react-native": "dist/native.js",
  "module": "dist/index.esm.js",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js",
    "react-native": "./dist/native.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/assets/images/*.png",
    "src/assets/images/*.jpg"
  ],
  "scripts": {
    "scripts": "node ./scripts/index.js",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --outDir dist",
    "build:rollup": "rollup -c",
    "build": "npm run build:types && npm run build:rollup",
    "publish:package": "npm run build && npm publish --access public",
    "release:patch": "npm version patch && npm run publish:package",
    "release:minor": "npm version minor && npm run publish:package",
    "release:major": "npm version major && npm run publish:package"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thegetcompany/assets.git"
  },
  "author": "Getgoing",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.2.0 <20.0.0",
    "react-dom": ">=18.2.0 <20.0.0",
    "react-native": ">=0.60.0",
    "react-native-svg": "^13.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@svgr/core": "^8.1.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "rimraf": "^6.0.1",
    "rollup": "^3.29.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "firebase": "^12.5.0",
    "rollup-plugin-copy": "^3.5.0",
    "sharp": "^0.34.4"
  }
}
